{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "preview": "vite preview",
    "list-tables": "node scripts/list-tables.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "db:reset": "npx supabase db reset",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:types": "npx supabase gen types typescript --local > src/types/database.types.ts",
    "db:schemas": "npx supazod -i src/types/database.types.ts -o src/lib/schemas/database.schemas.ts",
    "db:gen": "npm run db:types && npm run db:schemas"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.11.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
